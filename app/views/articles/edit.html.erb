
<h1>Edit article</h1>

<%= form_with scope: :article, model: @article, local: true do |form| %>

	<p>
		<%= form.label :author_name %><br>
		<%= form.text_field :author_name %>
	</p>

	<p>
		<%= form.label :photo_url %><br>
		<%= form.text_field :photo_url %>
	</p>

	<p>
		<%= form.label :title %><br>
		<%= form.text_field :title %>
	</p>

	<p>
		<%= form.submit %>
	</p>

<% end %>

<%= link_to 'Back', root_path %>