<%= form_with scope: :insta, url: '/insta', local: true do |form| %>
  <div class="form-group">
  <label for="newtitle">Title:</label>
    <%# <%= form.label :title, for: 'newtitle', value: 'Title' %>
    <%= form.text_field :title, id: 'newtitle', class: 'form-control', placeholder: 'Your title' %>
  </div>

  <div class="form-group">
    <%= form.label :caption %>
    <%= form.text_area :caption, class: 'form-control' %>
  </div>

    <%= form.submit "Create post", class: 'btn btn-primary' %>
<% end %>