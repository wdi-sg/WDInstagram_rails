<%= form_with scope: :picture, url: pictures_path, local: true do |form| %>

    <p>
    <%= form.label :author_name %><br>
    <%= form.text_area :author_name %>
    </p>

    <p>
      <%= form.label :photo_url %><br>
      <%= form.text_field :photo_url %>
    </p>
  
    <p>
      <%= form.label :title %><br>
      <%= form.text_area :title %>
    </p>

    <p>
    <%= form.label :captions %><br>
    <%= form.text_area :captions %>
  </p>

  <p>
  <%= form.label :gif_query %><br>
  <%= form.text_area :gif_query %>
</p>

    <p>
      <%= form.submit %>
    </p>
  <% end %>

  <%= link_to 'Back', root_path %>