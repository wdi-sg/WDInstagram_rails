<h1>WDInstagrams_Rails</h1>
<hr>
<div class="container">
	<ol>
	  <% @entries.each do |entry| %>
	    <li>
	      <p><%= entry.author %></p>
	      <p><%= entry.photo_url %></p>
	      <p><%= entry.date_taken %></p>
	      <p>
	      	<%= link_to 'Show', entry_path(entry) %>
		      <%= link_to 'Edit', edit_entry_path(entry) %>
		      <%= link_to 'Destroy', entry_path(entry),
	        method: :delete,
	        data: { confirm: 'Are you sure?' } %>
	      </p>
	    </li>
	  <% end %>
	</ol>
</div>