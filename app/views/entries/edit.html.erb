<h1>Edit entry</h1>

<%= form_with scope: :entry, model: @entry, local: true do |form| %>

	<p>
    <%= form.label :photo_url %><br>
    <%= form.text_field :photo_url %>
	</p>

  <p>
    <%= form.label :author %><br>
    <%= form.text_field :author %>
  </p>

	<p>
    <%= form.label :date_taken %><br>
    <%= form.text_field :date_taken %>
  </p>


  <p>
    <%= form.submit %>
  </p>

<% end %>

<%= link_to 'Back', entries_path %>
