<%= form_with scope: :photo, url: photos_path, local: true do |form| %>
  <p>
    <%= form.label :author %><br>
    <%= form.text_field :author %>
  </p>

  <p>
    <%= form.label :url %><br>
    <%= form.text_field :url %>
  </p>

   <p>
    <%= form.label :date %><br>
    <%= form.datetime_field :date %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>

<%= link_to 'Back', photos_path %>
