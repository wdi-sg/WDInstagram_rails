<%= form_with scope: :post, url: posts_path, local: true do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :photo %><br>
    <%= form.text_area :photo_url %>
  </p>

    <p>
    <%= form.label :title %><br>
    <%= form.text_field  :title %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>


