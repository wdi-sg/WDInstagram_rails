<h1>Edit Profile</h1>

<%= form_with scope: :video, model: @video, local: true do |form| %>

   <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :url %><br>
    <%= form.text_area :url %>
  </p>


  <p>
    <%= form.submit %>
  </p>
<% end %>

<%= link_to 'Back', root_path %>